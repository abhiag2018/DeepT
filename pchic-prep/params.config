params{
  dataDir="/projects/li-lab/agarwa/CUBE/DeepTact/dataset"

  // PCHi-C data processing options
  hic_input_hg = "$dataDir/PCHiC_data/PCHiC_peak_matrix_cutoff5.tsv"
